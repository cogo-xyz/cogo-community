{
  "schemaVersion": "1",
  "runner_type": "web",
  "allowlist": ["cogo_cli_flutter"],
  "denylist": [],
  "env_allowlist": ["^(SUPABASE|COGO)_"],
  "env_denylist": ["SECRET", "KEY$"],
  "auto_cli_allowlist": ["json-set", "json-merge", "json-remove"],
