{
  "annotations": { "list": [] },
  "title": "COGO â€“ Minimal Ops",
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "Queued / Succeeded / Failed (rps)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "targets": [
        { "expr": "rate(cogo_jobs_queued_total[5m])", "refId": "A" },
        { "expr": "rate(cogo_jobs_succeeded_total[5m])", "refId": "B" },
        { "expr": "rate(cogo_jobs_failed_total[5m])", "refId": "C" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Inflight & Concurrency",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 },
      "targets": [
        { "expr": "cogo_jobs_inflight", "refId": "A" },
        { "expr": "cogo_worker_concurrency", "refId": "B" }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP p50/p95 (ms)",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.5, sum(rate(cogo_http_request_duration_ms_bucket[5m])) by (le))", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(cogo_http_request_duration_ms_bucket[5m])) by (le))", "refId": "B" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Job p50/p95 (ms)",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.5, sum(rate(cogo_job_latency_ms_bucket[5m])) by (le))", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(cogo_job_latency_ms_bucket[5m])) by (le))", "refId": "B" }
      ]
    },
    {
      "type": "stat",
      "title": "Realtime events / Backoffs (rps)",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 6 },
      "targets": [
        { "expr": "rate(cogo_realtime_events_total[5m])", "refId": "A" },
        { "expr": "rate(cogo_job_backoff_total[5m])", "refId": "B" }
      ]
    }
  ],
  "templating": { "list": [] },
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","30s","1m","5m"] }
}


