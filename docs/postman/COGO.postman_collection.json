{
  "info": { "name": "COGO Core", "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json" },
  "item": [
    {
      "name": "chat-gateway",
      "item": [
        {
          "name": "chat",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Authorization", "value": "Bearer {{SUPABASE_ANON_KEY}}" },
              { "key": "apikey", "value": "{{SUPABASE_ANON_KEY}}" }
            ],
            "url": { "raw": "{{SUPABASE_URL}}/functions/v1/chat-gateway", "host": ["{{SUPABASE_URL}}"], "path": ["functions","v1","chat-gateway"] },
            "body": { "mode": "raw", "raw": "{\n  \"project_uuid\": \"{{PROJECT_UUID}}\",\n  \"messages\": [{\"role\": \"user\", \"content\": \"Hello\"}]\n}" }
          }
        }
      ]
    },
    {
      "name": "actionflow-compat",
      "item": [
        {
          "name": "compile",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Authorization", "value": "Bearer {{SUPABASE_ANON_KEY}}" },
              { "key": "apikey", "value": "{{SUPABASE_ANON_KEY}}" }
            ],
            "url": { "raw": "{{SUPABASE_URL}}/functions/v1/actionflow-compat/compat/actionflow/compile", "host": ["{{SUPABASE_URL}}"], "path": ["functions","v1","actionflow-compat","compat","actionflow","compile"] },
            "body": { "mode": "raw", "raw": "{\n  \"project_uuid\": \"{{PROJECT_UUID}}\",\n  \"flow_id\": \"demo\",\n  \"version\": \"1.0.0\",\n  \"flow\": {\n    \"id\": \"demo\",\n    \"version\": \"1.0.0\",\n    \"steps\": [{\n      \"actionType\": \"setProperty\",\n      \"target\": \"appData.msg\",\n      \"params\": { \"value\": \"OK\" }\n    }]\n  }\n}" }
          }
        },
        {
          "name": "run (SSE)",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Accept", "value": "text/event-stream" },
              { "key": "Authorization", "value": "Bearer {{SUPABASE_ANON_KEY}}" },
              { "key": "apikey", "value": "{{SUPABASE_ANON_KEY}}" }
            ],
            "url": { "raw": "{{SUPABASE_URL}}/functions/v1/actionflow-compat/compat/actionflow/run", "host": ["{{SUPABASE_URL}}"], "path": ["functions","v1","actionflow-compat","compat","actionflow","run"] },
            "body": { "mode": "raw", "raw": "{\n  \"project_uuid\": \"{{PROJECT_UUID}}\",\n  \"flow_id\": \"demo\",\n  \"version\": \"1.0.0\"\n}" }
          }
        }
      ]
    },
    {
      "name": "rag",
      "item": [
        {
          "name": "search",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Authorization", "value": "Bearer {{SUPABASE_ANON_KEY}}" },
              { "key": "apikey", "value": "{{SUPABASE_ANON_KEY}}" }
            ],
            "url": { "raw": "{{SUPABASE_URL}}/functions/v1/rag", "host": ["{{SUPABASE_URL}}"], "path": ["functions","v1","rag"] },
            "body": { "mode": "raw", "raw": "{\n  \"q\": \"CreateGo to COGO migration\",\n  \"limit\": 3\n}" }
          }
        }
      ]
    }
  ],
  "variable": [
    { "key": "SUPABASE_URL", "value": "http://localhost:54321" },
    { "key": "SUPABASE_ANON_KEY", "value": "" },
    { "key": "PROJECT_UUID", "value": "00000000-0000-4000-8000-000000000000" }
  ]
}


